{"_id":"yy5v9j5emxNTGSQK","name":"Mustelid (Reikwald Badger)","type":"creature","img":"modules/wfrp4e-laws-pet-shoppe/assets/icons/badger-portrait.webp","data":{"characteristics":{"ws":{"type":"number","label":"CHAR.WS","abrev":"CHARAbbrev.WS","initial":25,"modifier":0,"advances":0,"calculationBonusModifier":0,"value":25,"bonus":2,"cost":25},"bs":{"type":"Number","label":"CHAR.BS","abrev":"CHARAbbrev.BS","initial":0,"modifier":0,"advances":0,"calculationBonusModifier":0,"value":0,"bonus":0,"cost":25},"s":{"type":"Number","label":"CHAR.S","abrev":"CHARAbbrev.S","initial":20,"modifier":0,"advances":0,"calculationBonusModifier":0,"value":20,"bonus":2,"cost":25},"t":{"type":"Number","label":"CHAR.T","abrev":"CHARAbbrev.T","initial":25,"modifier":0,"advances":0,"calculationBonusModifier":0,"value":25,"bonus":2,"cost":25},"i":{"type":"Number","label":"CHAR.I","abrev":"CHARAbbrev.I","initial":30,"modifier":0,"advances":0,"calculationBonusModifier":0,"value":35,"bonus":3,"cost":25},"ag":{"type":"Number","label":"CHAR.Ag","abrev":"CHARAbbrev.Ag","initial":30,"modifier":0,"advances":0,"calculationBonusModifier":0,"value":30,"bonus":3,"cost":25},"dex":{"type":"Number","label":"CHAR.Dex","abrev":"CHARAbbrev.Dex","initial":0,"modifier":0,"advances":0,"calculationBonusModifier":0,"value":0,"bonus":0,"cost":25},"int":{"type":"Number","label":"CHAR.Int","abrev":"CHARAbbrev.Int","initial":10,"modifier":0,"advances":0,"calculationBonusModifier":0,"value":10,"bonus":1,"cost":25},"wp":{"type":"Number","label":"CHAR.WP","abrev":"CHARAbbrev.WP","initial":20,"modifier":0,"advances":0,"calculationBonusModifier":0,"value":20,"bonus":2,"cost":25},"fel":{"type":"Number","label":"CHAR.Fel","abrev":"CHARAbbrev.Fel","initial":0,"modifier":0,"advances":0,"calculationBonusModifier":0,"value":0,"bonus":0,"cost":25}},"status":{"wounds":{"type":"Number","label":"Wounds","abrev":"W","value":6,"max":6},"advantage":{"type":"Number","label":"Advantage","value":0,"max":10},"criticalWounds":{"type":"Number